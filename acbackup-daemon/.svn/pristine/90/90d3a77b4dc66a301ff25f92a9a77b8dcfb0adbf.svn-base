package sga.sol.ac.acotp.daemon.server;

import org.tinyradius.packet.RadiusPacket;

public class ResponseMessage {
	private RadiusPacket request;
	private RadiusPacket response;
	
	public ResponseMessage(RadiusPacket request, RadiusPacket response) {
		this.request = request;
		this.response = response;
	}

	public RadiusPacket getRequest() {
		return request;
	}

	public void setRequest(RadiusPacket request) {
		this.request = request;
	}

	public RadiusPacket getResponse() {
		return response;
	}

	public void setResponse(RadiusPacket response) {
		this.response = response;
	}
}
