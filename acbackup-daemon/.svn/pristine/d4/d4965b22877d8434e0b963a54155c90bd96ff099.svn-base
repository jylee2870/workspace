package sga.sol.ac.acotp.daemon.server;

public class ServerProperty {
	private String listenAddress;
	private String authPort;
	private String acctPort;
	
	private String nasIp;
	private String sharedSecret;
	
	private String issueValid;
	private String issueLimit;
	private String userLimit;
	private String gracePeriod;
	// 2018-08-28 jylee update
	private String totpWindowSize;
	// 2018-08-28 jylee update
		private String hotpWindowSize;
	
	private String accountLink;
	private String accountLock;
	
	public String getListenAddress() {
		return listenAddress;
	}
	public void setListenAddress(String listenAddress) {
		this.listenAddress = listenAddress;
	}
	public String getAuthPort() {
		return authPort;
	}
	public void setAuthPort(String authPort) {
		this.authPort = authPort;
	}
	public String getAcctPort() {
		return acctPort;
	}
	public void setAcctPort(String acctPort) {
		this.acctPort = acctPort;
	}

	public String getNasIp() {
		return nasIp;
	}
	public void setNasIp(String nasIp) {
		this.nasIp = nasIp;
	}
	public String getSharedSecret() {
		return sharedSecret;
	}
	public void setSharedSecret(String sharedSecret) {
		this.sharedSecret = sharedSecret;
	}
	
	public String getIssueValid() {
		return issueValid;
	}
	public void setIssueValid(String issueValid) {
		this.issueValid = issueValid;
	}
	public String getIssueLimit() {
		return issueLimit;
	}
	public void setIssueLimit(String issueLimit) {
		this.issueLimit = issueLimit;
	}
	public String getUserLimit() {
		return userLimit;
	}
	public void setUserLimit(String userLimit) {
		this.userLimit = userLimit;
	}
	public String getGracePeriod() {
		return gracePeriod;
	}
	public void setGracePeriod(String gracePeriod) {
		this.gracePeriod = gracePeriod;
	}

	public String getAccountLink() {
		return accountLink;
	}
	public void setAccountLink(String accountLink) {
		this.accountLink = accountLink;
	}
	public String getAccountLock() {
		return accountLock;
	}
	public void setAccountLock(String accountLock) {
		this.accountLock = accountLock;
	}
	public String getTotpWindowSize() {
		return totpWindowSize;
	}
	public void setTotpWindowSize(String totpWindowSize) {
		this.totpWindowSize = totpWindowSize;
	}
	public String getHotpWindowSize() {
		return hotpWindowSize;
	}
	public void setHotpWindowSize(String hotpWindowSize) {
		this.hotpWindowSize = hotpWindowSize;
	}
	
}
